{
  "version": 2,
  "builds": [
    {
      "src": "pitch.html",
      "use": "@vercel/static"
    },
    {
      "src": "app mockups/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/pitch.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
